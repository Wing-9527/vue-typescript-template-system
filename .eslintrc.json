{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended"],
  "ignorePatterns": ["node_modules"],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "vue/multi-word-component-names": 0
  }
}
